{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"target": "ESNext",
		"rootDir": "src",
		"outDir": "out",
		"lib": ["ESNext"],
		"strict": true,
		"noUnusedLocals": true,
		"esModuleInterop": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
